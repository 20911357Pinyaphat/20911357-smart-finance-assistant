# Define a reusable transaction structure
def create_transaction(id, date_str, description, amount, category, currency="AUD", source=None, recurring=False, notes="", tags=None):
    return {
        "id": id,
        "date": date_str,
        "description": description,
        "amount": amount,
        "currency": currency,
        "category": category,
        "source": source,
        "recurring": recurring,
        "notes": notes,
        "tags": tags or []
    }

# Create sample transactions
transactions = [
    create_transaction(
        id="txn_20251018_001",
        date_str="2025-10-18",
        description="Spotify Premium",
        amount=-11.99,
        category="Entertainment",
        source="Visa Debit",
        recurring=True,
        tags=["music", "subscription"]
    ),
    create_transaction(
        id="txn_20251017_002",
        date_str="2025-10-17",
        description="Salary October",
        amount=3200.00,
        category="Income",
        source="Bank Transfer",
        notes="Includes bonus"
    ),
    create_transaction(
        id="txn_20251016_003",
        date_str="2025-10-16",
