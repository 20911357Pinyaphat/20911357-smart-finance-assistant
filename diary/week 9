Entry [4] ‚Äì [improved transactions]

Artifact: [from datetime import datetime
from typing import List, Optional

# Allowed categories for consistency
ALLOWED_CATEGORIES = {
    "Groceries", "Entertainment", "Income", "Utilities", "Dining",
    "Shopping", "Transport", "Health", "Savings", "Investments"
}

# Helper: Parse and validate date
def parse_date(date_str: str) -> datetime.date:
    try:
        return datetime.strptime(date_str, "%Y-%m-%d").date()
    except ValueError:
        raise ValueError(f"Invalid date format: {date_str}. Use YYYY-MM-DD.")

# Helper: Validate category
def validate_category(category: str) -> str:
    if category not in ALLOWED_CATEGORIES:
        raise ValueError(f"Invalid category: '{category}'. Allowed: {sorted(ALLOWED_CATEGORIES)}")
    return category

# Helper: Infer transaction type
def infer_type(amount: float) -> str:
    return "income" if amount > 0 else "expense"

# Helper: Normalize source
def normalize_source(source: Optional[str]) -> Optional[str]:
    return source.lower().replace(" ", "_") if source else None

# Transaction factory
def create_transaction(
    id: str,
    date_str: str,
    description: str,
    amount: float,
    category: str,
    currency: str = "AUD",
    source: Optional[str] = None,
    recurring: bool = False,
    notes: str = "",
    tags: Optional[List[str]] = None
) -> dict:
    return {
        "id": id,
        "date": parse_date(date_str),
        "description": description.strip(),
        "amount": round(amount, 2),
        "currency": currency.upper(),
        "category": validate_category(category),
        "type": infer_type(amount),
        "source": normalize_source(source),
        "recurring": recurring,
        "notes": notes.strip(),
        "tags": tags or []
    }

# Sample transactions
transactions = [
    create_transaction(
        id="txn_20251018_001",
        date_str="2025-10-18",
        description="Spotify Premium",
        amount=-11.99,
        category="Entertainment",
        source="Visa Debit",
        recurring=True,
        tags=["music", "subscription"]
    ),
    create_transaction(
        id="txn_20251017_002",
        date_str="2025-10-17",
        description="Salary October",
        amount=3200.00,
        category="Income",
        source="Bank Transfer",
        notes="Includes bonus"
    ),
    create_transaction(
        id="txn_20251016_003",
        date_str="2025-10-16",
        description="Woolworths groceries",
        amount=-85.40,
        category="Groceries",
        source="Mastercard",
        tags=["food", "essentials"]
    )
]

# Print transactions
for txn in transactions:
    print(txn)
]

