def plot_spending(selected_services):
    all_services = selected_services + manual_services
    if not all_services:
        return None, "âš ï¸ No subscriptions selected ğŸ’…", "ğŸ’” Sparkle Score: 0/10"

    selected_df = df[df["Description"].isin(all_services)]
    if selected_df.empty:
        return None, "âš ï¸ No matching subscriptions found ğŸ’…", "ğŸ’” Sparkle Score: 0/10"

    breakdown = selected_df.groupby("Category")["Amount"].sum()

    import matplotlib.pyplot as plt
    import numpy as np

    # ğŸŒ¸ Gradient colors
    gradient_colors = ["#ff69b4", "#ff85c1", "#ff99cc", "#ffb6c1", "#ffc0cb", "#ffe6f0"]
    categories = breakdown.index
    values = breakdown.values

    fig, ax = plt.subplots(figsize=(8, 5))
    bars = ax.bar(categories, values, color=gradient_colors[:len(categories)], edgecolor="#ff69b4")

    # ğŸ€ Rounded tops
    for bar in bars:
        bar.set_linewidth(0)
        bar.set_alpha(0.9)
        bar.set_zorder(3)
        bar.set_capstyle("round")

    # ğŸ¬ Emoji labels
    emoji_map = {
        "Movies & TV": "ğŸ¬",
        "Music": "ğŸµ",
        "Food": "ğŸ”",
        "Fitness": "ğŸ’ª",
        "Shopping": "ğŸ›ï¸",
        "Internet & Phone": "ğŸ“¶"
    }
    ax.set_xticks(np.arange(len(categories)))
    ax.set_xticklabels([f"{emoji_map.get(cat, '')} {cat}" for cat in categories], rotation=45, ha="right")

    # âœ¨ Sparkle title and styling
    ax.set_title("âœ¨ Monthly Spend by Category âœ¨", fontsize=14, color="#ff69b4", weight="bold")
    ax.set_ylabel("Amount ($)", fontsize=12)
    ax.set_facecolor("#fff0f5")
    fig.patch.set_facecolor("#ffe6f0")
    plt.tight_layout()

    # ğŸ’¬ Summary
    total = selected_df["Amount"].sum()
    summary = f"ğŸ’– Total Monthly Spend: ${total:.2f}\n"
    for cat, amt in breakdown.items():
        summary += f"â€¢ {emoji_map.get(cat, '')} {cat}: ${amt:.2f}\n"

    # âœ¨ Sparkle Score
    sparkle = 10
    if total > 200:
        sparkle -= 3
    if "Movies & TV" in breakdown and breakdown["Movies & TV"] > 40:
        sparkle -= 2
    if "Music" in breakdown and breakdown["Music"] > 20:
        sparkle -= 1
    sparkle = max(sparkle, 0)
    sparkle_text = f"âœ¨ Sparkle Score: {sparkle}/10"

    return fig, summary, sparkle_text
